[core]
learner = average_perceptron
parser = ceisner
feature_generator = english_1st_fgen

[data]
format = format/Ontonotes.format
train  = conll_2012_english_train
test   = conll_2012_english_test
data_path = %(NATLANG_DATA)s/conll-2012-processed/
prep_path = data/prep/
tag_file = tagset.txt

[option]
h_flag = False
parallel_train = False
shards = 1
iteration = 5
l_filename =
d_filename =
timer = True
dump_frequency = 1
log-feature-request = False
interactive = False

