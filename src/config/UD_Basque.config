[core]
learner = average_perceptron
parser = ceisner
feature_generator = english_1st_fgen

[data]
format = format/conllu.format
train  = eu-ud-train.conllu
test   = eu-ud-dev.conllu
data_path = %(NATLANG_DATA)s/glm-parser-data/universal-dependencies-1.2/
prep_path = data/prep/
tag_file = tagset.txt

[option]
h_flag = False
parallel_train = False
shards = 1
iteration = 5
l_filename =
d_filename =
timer = True
dump_frequency = 1
log-feature-request = False
interactive = False
