[core]
learner = average_perceptron
parser = ceisner
feature_generator = english_1st_fgen

[data]
format = format/conllx.format
train  = dutch_alpino_train.conll
test   = dutch_alpino_test.conll
data_path = %(NATLANG_DATA)s/glm-parser-data/CoNLL-X/Dutch/data/
prep_path = data/prep/
tagger_w_vector = fv_Iter_1.db
tag_file = tagset.txt

[option]
spark_shards = 4
iterations = 5
load_weight_from =
dump_weight_to =
dump_frequency = 1
